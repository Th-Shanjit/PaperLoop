      "env": {
        "EXPO_PUBLIC_GEMINI_API_KEY": "YOUR_ACTUAL_API_KEY_HERE"
      }
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  }
}